<view id="header">
  <!--头像部分-->
  <view class="content">
    <image src="{{userInfo.avatarUrl}}"></image>
    <view class="nickname">{{userInfo.nickName}}
    </view>
    <view class="orther">{{userInfo.province}}</view>
  </view>
</view>
<view id="section">
    <!--横向标题-->
    <view class="flex-wrap">
        <view class="flex-item"wx:for="{{x_menus}}">
            <view>
                <image src="{{item.icon}}" class="icon"></image>
                    <view class="menus_title">
                    <text>{{item.title}}</text>
                    </view>
            </view>
        </view>
    </view>
    </view>
       <!--纵向标题-->
       <view id="menus">
        <view class="cont" wx:for="{{y_menus}}">
            <view class="view">
                <text>{{item.title}}</text>
            </view>
            <view class="right">
                <image src="../../img/rightjian.png"></image>
            </view>
        </view>
        </view>